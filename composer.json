{
  "name": "mattbromley/inertia-statamic",
  "description": "A fork of the Inertia.js Statamic adapter by Adam Campbell",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "inertia",
    "inertia.js",
    "statamic",
    "adapter"
  ],
  "authors": [
    {
      "name": "Adam Campbell",
      "email": "adam@hotmeteor.com"
    },
    {
      "name": "Matthew Bromley",
      "email": "noreply@tenantce.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "inertiajs/inertia-laravel": "^v2.0.1",
    "illuminate/support": "^v10.42.0",
    "statamic/cms": "5.17.0"
  },
  "require-dev": {
    "nunomaduro/collision": "^5.1|^6.0",
    "orchestra/testbench": "^v8.17.0",
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Hotmeteor\\Inertia\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hotmeteor\\Inertia\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  }
}
